    {
      "name": "b7884464",
      "displayName": "Compute Cluster Information - Summary",
      "queryLines": [
        "\r\n",
        "SELECT DISTINCT\r\n",
        "        c.cluster_id,\r\n",
        "        c.cluster_name,\r\n",
        "        concat  ('<a href=\"', \r\n",
        "                'https://dbc-8c09cf6c-96ac.cloud.databricks.com/sql/dashboardsv3/01f0be1f772d1b39a95260361e24170f/pages/55d4c6cd?o=1011890501861229&f_e6e031ae=&f_55d4c6cd%7E541d9f07=',\r\n",
        "                url_encode(c.cluster_name),\r\n",
        "                '\">', c.cluster_name, '</a>'\r\n",
        "                ) as cluster_name_link, --Update this link\r\n",
        "        c.delete_time,\r\n",
        "        c.driver_node_type,\r\n",
        "        c.worker_node_type,\r\n",
        "        c.worker_count,\r\n",
        "        c.auto_termination_minutes,\r\n",
        "        c.cluster_source,\r\n",
        "        min(n.start_time) as first_start,\r\n",
        "        max(n.end_time) as last_start,\r\n",
        "        w.workspace_name\r\n",
        "    FROM system.compute.clusters AS c\r\n",
        "LEFT JOIN system.compute.node_timeline AS n\r\n",
        "    ON c.cluster_id = n.cluster_id\r\n",
        "LEFT JOIN system.access.workspaces_latest as w\r\n",
        "    ON w.workspace_id = n.workspace_id\r\n",
        "WHERE driver = TRUE\r\n",
        "AND   (:filterDeleted = 'No' or c.delete_time IS NULL)\r\n",
        "Group by    c.cluster_id,\r\n",
        "            c.cluster_name,\r\n",
        "            c.delete_time,\r\n",
        "            c.driver_node_type,\r\n",
        "            c.worker_node_type,\r\n",
        "            c.worker_count,\r\n",
        "            c.auto_termination_minutes,\r\n",
        "            c.cluster_source,\r\n",
        "            w.workspace_name\r\n",
        "order by cluster_name\r\n",
        "\r\n"
      ],
      "parameters": [
        {
          "displayName": "filterDeleted",
          "keyword": "filterDeleted",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "Yes"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "8a4438f3",
      "displayName": "Simple Response Data - Yes and No",
      "queryLines": [
        "SELECT 'Yes' AS `Y/N`\r\n",
        "UNION ALL\r\n",
        "SELECT 'No' AS  `Y/N`"
      ]
    }
  ],
